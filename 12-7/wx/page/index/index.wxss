page{
  height:100%;
}
.container{
  width: 750rpx;
  height: 100%;
  background:#f5f5f5;
}
text.title{
  margin-top:200rpx;
  color: #368;
  font-size: 30px;
  font-weight: bold;
}
.search{
  width:80%;
  height: 90rpx;
}
.search-ipt{
  width: 75%;
  box-shadow: 0 0 1px #368;
  border-radius:10px;
  padding: 0 10px;
}

.search-tap{
  width: 100%;
  color:red;
}

scroll-view{
  width: 100%;
  height: 600rpx;
  background: #fff;
  border-top:4px solid #368;
}

scroll-view text{
  width: 100%;
  display: block
}

.items{
  padding: 20rpx;
  border-bottom: 2px solid #368;
}

.items text{
  font-size:14px;
  color:orangered;
  font-weight: bold;
  line-height: 60rpx;
}

.items text:nth-child(2){
  line-height: 50rpx;
  color: #999;
  font-size: 12px;
  text-shadow: 0 0 #368;
}